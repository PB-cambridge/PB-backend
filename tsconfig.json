{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "./build",
		"sourceMap": true,
		"esModuleInterop": true,
		"strict": true,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"allowJs": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*" /*  "prisma/index.ts", "prisma/seed.ts", "index.ts" */]
}
